{
  "name": "databox-collector",
  "version": "1.0.0",
  "description": "Serves as an integration service between various data sources and Databox.",
  "main": "index.js",
  "scripts": {
    "compile": "tsc",
    "start": "node ./build/index.js",
    "start:dev": "tsc-watch --onSuccess \"node ./dist/src/index.js\"",
    "clean": "rm -rf node_modules dist",
    "test": "jest",
    "prepare": "husky install",
    "pre-commit": "lint-staged && npm run test",
    "lint": "eslint --fix --ext .ts ."
  },
  "author": "Rok Pusnik",
  "license": "ISC",
  "devDependencies": {
    "@types/cron": "2.0.0",
    "@types/express": "4.17.14",
    "@types/jest": "29.2.3",
    "@types/node": "18.11.10",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "lint-staged": "13.0.4",
    "prettier": "2.8.0",
    "ts-jest": "^29.0.3",
    "tsc-watch": "5.0.3",
    "typescript": "4.9.3"
  },
  "dependencies": {
    "axios": "1.2.0",
    "cron": "2.1.0",
    "databox": "2.0.1",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "express-promise-router": "4.1.1"
  },
  "lint-staged": {
    "(src|test)/**/*.ts": [
      "eslint --fix"
    ]
  }
}